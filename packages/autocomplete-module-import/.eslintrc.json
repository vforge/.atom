{
    "env": {
        "es6": true,
        "node": true,
        "atomtest": true
    },
    "globals": {
        "atom": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2017
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
        "prettier/prettier": ["error", {"singleQuote": true}]
    }
}
